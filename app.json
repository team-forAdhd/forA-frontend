{
    "expo": {
        "name": "forA",
        "slug": "forA",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "src/public/assets/forA-icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "src/public/assets/forA-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "ios.forA",
            "config": {
                "googleMapsApiKey": ""
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "src/public/assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "permissions": ["ACCESS_FINE_LOCATION"]
        },
        "web": {
            "favicon": "src/public/assets/favicon.png"
        }
    }
}
