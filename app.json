{
    "expo": {
        "name": "forA",
        "slug": "forA",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "src/public/assets/forA-icon.png",
        "userInterfaceStyle": "light",
        "plugins": [
            [
                "expo-splash-screen",
                {
                    "image": "src/public/assets/splash-n-onboard/splash.png",
                    "resizeMode": "contain",
                    "backgroundColor": "#52A55D"
                }
            ]
        ],
        "splash": {
            "image": "src/public/assets/splash-n-onboard/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#52A55D"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "ios.forA",
            "config": {
                "googleMapsApiKey": ""
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "src/public/assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "permissions": ["ACCESS_FINE_LOCATION"],
            "package": "ios.forA"
        },
        "web": {
            "favicon": "src/public/assets/favicon.png"
        }
    }
}
